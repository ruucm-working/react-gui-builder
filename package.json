{
  "name": "starter-app",
  "version": "0.0.1",
  "description": "Starter App",
  "engines": {
    "npm": ">=3"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "init-st": "rm -rf .structor && npm install structor",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "dev": "structor -p 2223",
    "deploy": "npm run build && bash deploy.sh"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "chalk": "1.1.3",
    "compression": "^1.7.2",
    "cross-env": "3.1.3",
    "expect": "1.20.2",
    "express": "^4.16.3",
    "ip": "1.1.3",
    "lodash": "^4.17.10",
    "minimist": "1.2.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-gui-builder": "0.0.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-scroll": "0.3.3",
    "redux": "3.6.0",
    "redux-saga": "0.12.0",
    "reselect": "2.5.4",
    "ruucm-blocks": "github:ruucm/ruucm-blocks#master",
    "ruucm-util": "^0.10.0",
    "styled-components": "3.3.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cheerio": "^0.22.0",
    "css-loader": "0.25.0",
    "eslint": "3.9.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-config-airbnb-base": "9.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eslint-plugin-redux-saga": "0.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "lint-staged": "3.2.0",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.0"
  }
}
